<Window x:Class="Авто_Ресурс_Сервис.Windows.SaveNesInfoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Авто_Ресурс_Сервис.Windows"
        mc:Ignorable="d"
        Title="SaveNesInfoWindow" Height="250" Width="400">
    <Grid>
        <Button x:Name="Save" Content="Зберети" HorizontalAlignment="Center" Margin="0,167,0,0" VerticalAlignment="Top" Height="37" Width="120" Click="Save_Click"/>
        <ComboBox Name="TypeMode" HorizontalAlignment="Center" Margin="0,48,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="TypeMode_SelectionChanged"/>
        <TextBox x:Name="TypeModeSelect" HorizontalAlignment="Center" Margin="0,91,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
